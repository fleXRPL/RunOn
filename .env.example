# Google API Key
RUNON_API_KEY=your_api_key_here

# Google Custom Search Engine ID
RUNON_SEARCH_ENGINE_ID=your_search_engine_id_here

# Google Client ID
RUNON_CLIENT_ID=your_client_id_here.apps.googleusercontent.com

# Google Client Secret
RUNON_CLIENT_SECRET=your_client_secret_here

# Additional Recommended Settings
ENVIRONMENT=development
DEBUG=True
LOG_LEVEL=DEBUG

# JWT Settings
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# Redis Cache Settings (if using)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DB=0

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60 